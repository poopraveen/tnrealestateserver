{
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "build": {
        "env": {
            "TS_NODE_TRANSPILE_ONLY": "true"
        }
    },
    "hooks": {
    "build": "npm run build"
  }
}